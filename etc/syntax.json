        "type": "variable",
        "pattern": "var = \"(.*)\";",
        "replacement": "let mut \\1 = String::from(\"\\1\");"
    },
    "var_number": {
        "type": "variable",
        "pattern": "var = (\\d+);",
        "replacement": "let mut var = \\1;"
    },
    "ref": {
        "type": "input",
        "pattern": "ref (\\w+) >> (\\w+);",
        "replacement": "std::io::stdin().read_line(&mut \\2).unwrap();"
    },
    "int": {
        "type": "function",
        "pattern": "int (\\w+) \\{",
        "replacement": "fn \\1() {"
    }
